<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.blogfy.mapper.UserGithubInfoMapper" >

    <select id="getGithubInfoByGithubId" resultType="net.blogfy.entity.UserGithubInfo">
        select * from t_user_github_info where github_id = #{githubId}
    </select>


</mapper>